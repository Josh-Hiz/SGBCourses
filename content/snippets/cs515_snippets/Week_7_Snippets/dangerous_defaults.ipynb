    "def rev(l, acc=[]):\n",
    "    for x in l:\n",
    "        acc.insert(0, x)\n",
    "        \n",
    "    return acc\n",
    "\n",
    "print(rev([1,2,3]))\n",
    "print(rev(list(\"abc\")))"
